{
  "name": "enji-sdk",
  "version": "0.1.2",
  "author": "Codebrain <tech@codebrain.se>",
  "description": "An ECMAScript wrapper SDK around the Enji API",
  "scripts": {
    "build": "rm -rf dist && tsup",
    "release": "release-it",
    "lint": "eslint ."
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "require": "./dist/client/index.js",
      "import": "./dist/client/index.mjs"
    }
  },
  "release-it": {
    "git": {
      "requireCleanWorkingDir": true,
      "tagName": "v${version}"
    },
    "npm": {
      "publish": false
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.1.0",
    "eslint": "^9.32.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "release-it": "^19.0.4",
    "tsup": "^8.5.0",
    "typescript-eslint": "^8.38.0"
  },
  "dependencies": {
    "server-only": "^0.0.1"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.53.3"
  }
}
